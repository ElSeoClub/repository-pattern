{
    "name": "elseoclub/laravel-repository-pattern",
    "description": "A Laravel package for generating repository classes, interfaces, and service providers to implement the repository pattern efficiently",
    "keywords": [
        "laravel",
        "repository",
        "commands",
        "repository-pattern"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Ballesteros Huazano",
            "email": "elseoclub@gmail.com",
            "homepage": "https://github.com/elseoclub",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Elseoclub\\RepositoryPattern\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elseoclub\\RepositoryPattern\\Providers\\ModelRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
